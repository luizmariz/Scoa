<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.text.Font?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<?import javafx.scene.layout.Pane?>

<Pane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.components.MenuBtn">
   <JFXButton contentDisplay="CENTER" ellipsisString="" layoutX="997.0" layoutY="18.0" onAction="#handleMinimizeButtonAction" textFill="TRANSPARENT" textOverrun="CLIP">
      <font>
         <Font size="1.0" />
      </font>
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
      <graphic>
         <FontIcon id="btn_window_minimize" fill="#9e549b" iconLiteral="far-window-minimize" iconSize="17" />
      </graphic>
   </JFXButton>
   <JFXButton contentDisplay="CENTER" ellipsisString="" layoutX="1037.0" layoutY="18.0" onAction="#handleCloseButtonAction" textFill="TRANSPARENT" textOverrun="CLIP">
      <graphic>
         <FontIcon id="btn_window_close" fill="#9e549b" iconLiteral="far-window-close" iconSize="17" />
      </graphic>
      <font>
         <Font size="1.0" />
      </font>
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
   </JFXButton>
</Pane>


